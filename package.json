{
  "name": "@txo-peer-dep/i18n-react",
  "version": "4.0.64",
  "description": "Technology Studio - I18n react",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/i18n-react-peer.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint:ci && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh",
    "lint:ci": "yarn lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@txo/log": "^2.0.16",
    "@txo/types": "^1.7.0",
    "numbro": "^2.5.0"
  },
  "peerDependencies": {
    "i18next": "^23.16.8",
    "react-i18next": "^15.4.0"
  },
  "devDependencies": {
    "@txo-peer-dep/log": "^4.0.4",
    "@txo/commitlint": "^1.0.21",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^2.0.10",
    "@txo/tsconfig-cjs": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "concurrently": "^9.1.2",
    "eslint-config-txo-typescript-react": "^2.0.82",
    "husky": "^9.1.7",
    "i18next": "^23.16.8",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "npm-check-updates": "^17.1.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "redux-saga": "^1.3.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "@types/react": "^18.3.18"
  }
}
