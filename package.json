{
  "name": "@txo-peer-dep/i18n-react",
  "version": "4.0.27",
  "description": "Technology Studio - I18n react",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/i18n-react-peer.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "dependencies": {
    "@txo/log": "^2.0.14",
    "@txo/types": "^1.7.0",
    "numbro": "^2.4.0"
  },
  "peerDependencies": {
    "i18next": "^23.10.1",
    "react-i18next": "^14.1.0"
  },
  "devDependencies": {
    "@txo-peer-dep/log": "^4.0.3",
    "@txo/commitlint": "^1.0.14",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^1.0.8",
    "@txo/tsconfig": "^1.1.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.22",
    "concurrently": "^8.2.2",
    "eslint-config-txo-typescript-react": "^2.0.48",
    "husky": "^8.0.3",
    "i18next": "^23.10.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "redux-saga": "^1.3.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "resolutions": {
    "@types/react": "^18.2.70"
  }
}
